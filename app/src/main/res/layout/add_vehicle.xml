<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/lr270171b17666666"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <ImageView
                android:id="@+id/riz1elq9hmsq"
                android:layout_width="48dp"
                android:layout_height="30dp"
                android:layout_marginTop="26dp"
                android:layout_marginBottom="20dp"
                android:layout_marginStart="25dp"
                android:src="@drawable/back"
                 />
            <LinearLayout
                android:id="@+id/r4ydhyhvf7jv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="20sp"
                android:layout_marginHorizontal="22dp"

                android:orientation="vertical">
                <LinearLayout
                    android:id="@+id/romgjdd7kqak"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/cr52baeb1b9"
                    android:paddingVertical="30dp"
                    android:layout_marginHorizontal="3dp"

                    android:orientation="vertical">
                    <ImageView
                        android:id="@+id/r991odudhxya"
                        android:layout_width="105dp"
                        android:layout_height="104dp"
                        android:layout_marginBottom="26dp"
                        android:layout_marginStart="119dp"
                        android:src="@drawable/add_image"
                        />

                            <LinearLayout
                                android:id="@+id/ri0nlaqe8zgb"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="17dp"
                                android:layout_marginHorizontal="31dp"
                                android:clipChildren="false"
                                android:clipToPadding="false"
                                android:orientation="horizontal">
                                <EditText
                                    android:inputType="text"
                                    android:hint="Bike name"
                                    android:id="@+id/rtjqal6c8ut"
                                    android:textColor="#FFFFFF"
                                    android:textSize="16sp"
                                    android:layout_width="match_parent"
                                    android:layout_height="48dp"
                                    android:background="@drawable/cr12be9e4e4c4"
                                    android:paddingVertical="16dp"
                                    android:paddingHorizontal="15dp"
                                    />
                            </LinearLayout>
                            <EditText
                                android:inputType="text"
                                android:hint="Registration Number"
                                android:id="@+id/r2qvaj6nteki"
                                android:textColor="#FFFFFF"
                                android:textSize="16sp"
                                android:layout_marginBottom="17dp"
                                android:layout_marginHorizontal="31dp"
                                android:layout_width="match_parent"
                                android:layout_height="48dp"
                                android:background="@drawable/cr12be9e4e4c4"
                                android:paddingVertical="16dp"
                                android:paddingHorizontal="15dp"
                                />
                            <LinearLayout
                                android:id="@+id/rz8324qkk7x"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="17dp"
                                android:layout_marginHorizontal="31dp"
                                android:clipChildren="false"
                                android:clipToPadding="false"
                                android:orientation="horizontal">

                                <EditText
                                    android:inputType="text"
                                    android:hint="Milage"
                                    android:id="@+id/rlcq1i21qfsg"
                                    android:textColor="@color/white"
                                    android:textSize="16sp"
                                    android:layout_width="120dp"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/cr12be9e4e4c4"
                                    android:paddingVertical="16dp"
                                    android:layout_weight="0.5"
                                    android:paddingHorizontal="13dp"
                                    />
                                <EditText
                                    android:inputType="text"
                                    android:hint="Model"
                                    android:id="@+id/rtdwzw2ct8oe"
                                    android:textColor="#FFFFFF"
                                    android:textSize="16sp"
                                    android:layout_width="120dp"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/cr12be9e4e4c4"
                                    android:paddingVertical="16dp"
                                    android:layout_weight="0.5"
                                    android:layout_marginLeft="15dp"
                                    android:paddingHorizontal="13dp"
                                    />
                            </LinearLayout>
                    <LinearLayout
                        android:id="@+id/rz8324qkk7xe"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="17dp"
                        android:layout_marginHorizontal="31dp"
                        android:clipChildren="false"
                        android:clipToPadding="false"
                        android:orientation="horizontal">

                        <EditText
                            android:inputType="text"
                            android:hint="Colour"
                            android:id="@+id/rlcq1i21qfs"
                            android:textColor="#FFFFFF"
                            android:textSize="16sp"
                            android:layout_width="120dp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/cr12be9e4e4c4"
                            android:paddingVertical="16dp"
                            android:layout_weight="0.5"
                            android:paddingHorizontal="13dp"
                            />
                        <EditText
                            android:inputType="text"
                            android:hint="Rent/hour"
                            android:id="@+id/rtdwzw2ct8o"
                            android:textColor="#FFFFFF"
                            android:textSize="16sp"
                            android:layout_width="120dp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/cr12be9e4e4c4"
                            android:paddingVertical="16dp"
                            android:layout_weight="0.5"
                            android:layout_marginLeft="15dp"
                            android:paddingHorizontal="13dp"
                            />
                    </LinearLayout>

                    <Spinner
                        android:id="@+id/spinner_category"
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
                        android:layout_marginHorizontal="31dp"
                        android:layout_marginBottom="10dp"
                        android:background="@drawable/cr12be9e4e4c4"
                        android:paddingHorizontal="14dp"
                        android:paddingVertical="15dp"
                        android:textSize="16sp"/>
                    <TextView
                        android:textColor="@color/white"
                        android:layout_width="wrap_content"
                        android:layout_marginStart="20dp"
                        android:layout_height="wrap_content"
                        android:paddingVertical="16dp"
                        android:paddingHorizontal="13dp"
                        android:layout_marginHorizontal="10dp"
                        android:hint="*Fille the Availablility date and time correctly"
                        android:textColorHint="@color/red"
                        android:textSize="16sp" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:layout_marginBottom="10dp"
                        android:orientation="horizontal">

                        <EditText
                            android:textColor="@color/white"
                            android:layout_width="142dp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/cr12be9e4e4c4"
                            android:paddingVertical="16dp"
                            android:paddingHorizontal="13dp"
                            android:id="@+id/from_date"
                            android:layout_marginHorizontal="10dp"
                            android:hint="From Date"
                            android:onClick="showDatePicker"
                            android:focusable="false"
                            android:clickable="true"
                            android:textSize="16sp" />

                        <EditText


                            android:textColor="@color/white"
                            android:textSize="16sp"
                            android:layout_width="142dp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/cr12be9e4e4c4"
                            android:paddingVertical="16dp"
                            android:paddingHorizontal="13dp"
                            android:id="@+id/from_time"
                            android:hint="From time "
                            android:onClick="showTimePicker"
                            android:focusable="false"
                            android:clickable="true"
                            android:inputType="none" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:layout_marginBottom="10dp"
                        android:orientation="horizontal">

                        <EditText
                            android:textColor="@color/white"
                            android:layout_width="142dp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/cr12be9e4e4c4"
                            android:paddingVertical="16dp"
                            android:paddingHorizontal="13dp"
                            android:id="@+id/to_date"
                            android:layout_marginHorizontal="10dp"
                            android:hint="To Date"
                            android:focusable="false"
                            android:clickable="true"
                            android:onClick="showDatePicker"
                            android:textSize="16sp" />

                        <EditText


                            android:textColor="@color/white"
                            android:textSize="16sp"
                            android:layout_width="142dp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/cr12be9e4e4c4"
                            android:paddingVertical="16dp"
                            android:paddingHorizontal="13dp"
                            android:id="@+id/to_time"
                            android:hint="To time "
                            android:onClick="showTimePicker"
                            android:focusable="false"
                            android:clickable="true"
                            android:inputType="none" />
                    </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <Button
                    android:id="@+id/ryg6wnehzhhg"
                    android:textColor="@color/white"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    android:layout_gravity="center"
                    android:backgroundTint="@color/green"
                    android:layout_marginHorizontal="29dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="30dp"
                    android:layout_marginEnd="30dp"
                    android:layout_width="wrap_content"
                    android:text="Add Vehicle"
                    android:layout_marginBottom="30dp"
                    />
</LinearLayout>
    </ScrollView>
</LinearLayout>
